{
	"schemaVersion": 1,
	"id": "stuffmod",
	"version": "${version}",
	"name": "Stuff Mod",
	"description": "Custom Mod to add a bunch of stuff to Minecraft.",
	"authors": [
		"Thomas"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/stuffmod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.thomas.stuffmod.StuffMod"
		],
		"fabric-datagen": [
			"net.thomas.stuffmod.StuffModDataGenerator"
		],
		"client": [
			"net.thomas.stuffmod.StuffModClient"
		]
	},
	"mixins": [
		"stuffmod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.1",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}